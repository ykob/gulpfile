mixin svg(width, widthBase, heightBase)
  svg(
    xmlns = "http://www.w3.org/2000/svg",
    role = (attributes.role) ? attributes.role : 'img',
    aria-labelledby != `${attributes.id}-title`,
    width = width,
    height = Math.ceil(width * heightBase / widthBase),
    viewBox = `0 0 ${widthBase} ${heightBase}`,
    id != attributes.id,
    class != attributes.class,
  )
    if (attributes.title)
      title(
        id = `${attributes.id}-title`
        )
        |#{attributes.title}
    block

